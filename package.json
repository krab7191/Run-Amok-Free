{
  "name": "generic-tasting-app",
  "version": "1.0.0",
  "description": "A generic tasting app for alcoholic beverages of any kind, with an admin page that can be tailored to any business&#39;s needs.",
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "seed-users": "node server/scripts/seedUsers.js",
    "seed-teams": "node server/scripts/seedTeams.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/starrlightdev/generic-tasting-app.git"
  },
  "author": "Karsten Rabe",
  "license": "ISC",
  "homepage": "https://bitbucket.org/starrlightdev/generic-tasting-app#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^2.0.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "mongoose": "^5.4.4",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9"
  }
}